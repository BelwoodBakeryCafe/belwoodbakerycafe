{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "indent": 0,
        "no-mixed-spaces-and-tabs": 0,
        "no-undef": 0,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "no-plusplus": 0,
        "react/prefer-stateless-function": 0,
        "eol-last": 0,
        "global-require": 0
    }
}